{
  "name": "multistepcheckout",
  "version": "1.0.0",
  "description": "a single-page app that takes a user through a series of forms that simulate a shopping cart checkout experience.",
  "main": "queries.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit ",
    "start:server": "nodemon server.js",
